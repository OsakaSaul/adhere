{
  "name": "camera-on",
  "version": "1.0.0",
  "description": "A discord bot that enforces a camera on rule for voice chat users.",
  "main": "main.js",
  "scripts": {
    "build": "rm -rf ./dist && npm run tsc",
    "dev": "tsx ./src/main.ts",
    "start": "node -r dotenv/config dist/main.js",
    "tsc": "tsc",
    "postinstall": "npm run build",
    "deployCommands": "yarn build && node -r dotenv/config dist/deploy-commands.js"
  },
  "author": "davis9001",
  "license": "ISC",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "@ffmpeg/core": "^0.12.4",
    "@ffmpeg/ffmpeg": "^0.12.7",
    "@hokify/agenda": "^6.3.0",
    "@tsconfig/node18": "^18.2.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.11",
    "mongodb": "^5.7.0",
    "tsc": "^2.0.4",
    "tsconfig": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
